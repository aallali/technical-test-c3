		{
			"name": "login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Result have the expected properties\", function () {",
							"    var response = pm.response.json();",
							" ",
							"    pm.expect(response.tokens.access).to.be.not.null;",
							"    pm.expect( response.tokens.refresh).to.be.not.null;",
							"",
							"    // Get the admin token and put it in environment variables",
							"    pm.environment.set(\"jwToken\", response.tokens.access);",
							"    pm.environment.set(\"refreshToken\", response.tokens.refresh);",
							"",
							"    ",
							"});",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
